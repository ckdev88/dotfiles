{
  "biome.enable": true,
  "coc.preferences.formatOnSave": false,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.messageDelay": 800,
  "diagnostic.messageTarget": "float",
  "diagnostic.virtualText": false,
  "diagnostic.virtualTextAlign": "right",
  "diagnostic.virtualTextCurrentLineOnly": false,
  "diagnostic.virtualTextFormat": "%message (%source%code)",
  "html.format.enable": true,
  "javascript.format.enable": true,
  "pyright.enable": true,
  "pyright.inlayHints.functionReturnTypes": false,
  "pyright.inlayHints.parameterTypes": false,
  "pyright.inlayHints.variableTypes": false,
  "python.formatting.provider": "autopep8",
  "scss.format.enable": true,
  "suggest.enablePreselect": true,
  "suggest.noselect": false,
  "suggest.virtualText": true,
  "tailwindCSS.emmetCompletions": false,
  "tailwindCSS.enable": false,
  "tailwindCSS.trace.server": "verbose",
  "typescript.format.enable": false,
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.preferences.includePackageJsonAutoImports": "auto",
  "snippets.ultisnips.enable": true,
  "snippets.extends": {
    "all": ["ultisnips"]
  },
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": false,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": false,
  "volar.disableDiagnostics": false,
  "volar.disableFormatting": false,
  "volar.enable": true,
  "vetur.enable": true,
  "vue.format.script.initialIndent": false,
  "vue.format.style.initialIndent": true,
  "vue.format.template.initialIndent": true,
  "vue.inlayHints.destructuredProps": true,
  "vue.inlayHints.missingProps": false,
  "vue.inlayHints.optionsWrapper": false,
  "vue.inlayHints.vBindShorthand": false,
  "tsserver.enable": true,
  "tsserver.enableTracing": true,
  "languageserver": {
    "csharp-ls": {
      "command": "csharp-ls",
      "filetypes": ["cs"],
      "rootPatterns": ["*.csproj", ".vim/", ".git/", ".hg/"]
    },
    "biome": {
      "command": "biome",
      "args": ["lsp-proxy"],
      "filetypes": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "json",
        "jsonc",
        "html"
      ],
      "rootPatterns": ["biome.json"],
      "requireRootPattern": false,
      "trace.server.verbosity": "verbose"
    }
  },
  "javascript.suggest.autoImports": true,
  "typescript.suggest.autoImports": true,
  "workspace.workspaceFolderCheckCwd": false
}
